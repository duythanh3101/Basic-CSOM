<Window x:Class="PermissionTraning.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PermissionTraning"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button Content="Assign permission" Click="AssignPermission_Click" Height="30" Margin="10"/>
            <Button Content="Delete permission" Click="DeletePermission_Click" Height="30" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
